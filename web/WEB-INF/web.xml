        <servlet-name>findFSA</servlet-name>
        <url-pattern>/findFSA.safe</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>findLocation</servlet-name>
        <url-pattern>/findLocation.safe</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cancelProgress</servlet-name>
        <url-pattern>/cancelProgress.safe</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>maxInactiveIntervalProcess</servlet-name>
        <url-pattern>/maxInactiveIntervalProcess.safe</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetTileSpectrum</servlet-name>
        <url-pattern>/getTileSpectrum.safe</url-pattern>
    </servlet-mapping>
